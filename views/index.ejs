<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %> 
<body>
    <%- include('./partials/nav.ejs') %> 

    <div class="blogs content">
        <h2>All blogs</h2>
        <% if (blogs.length > 0) { %>
            <% blogs.forEach(blog => { %>
                <h3 class="title">
                    <%= blog.title %>
                </h3>
                <p>
                    <%= blog.snippet %>
                </p>
            <%}); %>
        <%} %>
        
    </div>

    <%- include('./partials/footer.ejs') %> 
</body>
</html>